[push]
	default = simple
[alias]
	st = status
            z = cherry-pick
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[commit]
	verbose = true
[pretty]
            arp = %C(auto,green)%h  %C(auto,cyan)%ad  %C(auto,bold yellow)%s
